{
    "name": "conservationafrica/laminas-money",
    "description": "Helpful form and validation stuff for Laminas form and moneyphp",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.3.99"
        }
    },
    "require": {
        "php": "^7.3 || ~8.0 || ~8.1",
        "ext-json": "*",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-form": "^2.16 || ^3.0",
        "laminas/laminas-hydrator": "^3 || ^4",
        "laminas/laminas-i18n": "^2.9",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-servicemanager": "^3",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-validator": "^2.13",
        "moneyphp/money": "^3.3.3 || ^4.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^11.1",
        "laminas/laminas-code": "^3.5.1 || ^4.0.0",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-db": "^2.15.1",
        "phpunit/phpunit": "^9.6.9",
        "psalm/plugin-phpunit": "^0.18.4",
        "vimeo/psalm": "^4.30"
    },
    "autoload": {
        "psr-4": {
            "ACE\\Money\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ACETest\\Money\\": "test/Money"
        }
    }
}
